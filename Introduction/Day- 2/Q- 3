#include <stdio.h>
int main() {
    int n, pos, i;
    printf("enter the no. of elements: ");
    scanf("%d", &n);
    int arr[n];
    printf("enter the elements of your array: ");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    printf("enter the position you want the element to be deleted: ");
    scanf("%d", &pos);

    for (i = pos - 1; i < n - 1; i++) {
        arr[i] = arr[i + 1];
    }
    n--;

    for (i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }
    return 0;
}
